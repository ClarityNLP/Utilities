include ClarityCore version "1.0" called Clarity;

documentset Docs:
    Clarity.createReportTypeList(["Nursing/other", "Nursing", "Physician ", "Discharge summary", "Radiology",
         "General", "ECG", "Pathology", "Echo"]);

// Medication(s) inclusion criteria termset, if medication criteria present

termset TreatmentTerms0:[
    "onivyde", "irinotecan liposome"
];

define Treatment_Irinotecanliposome:
    Clarity.ProviderAssertion({
        termset:[TreatmentTerms0],
        documentset:[Docs]
}); 

        

// Regimen names

termset RegimenTerms:[
    "Irinotecan_liposomal_monotherapy", "Irinotecan liposomal monotherapy", "Irinotecanliposomalmonotherapy"
];

define Regimen_Irinotecanliposomalmonotherapy:
  Clarity.ProviderAssertion({
    termset:[RegimenTerms],
    documentset:[Docs]
   }); 


// Results

        
define final Received_Irinotecanliposomalmonotherapy:
    where Treatment_Irinotecanliposome OR Regimen_Irinotecanliposomalmonotherapy;
            


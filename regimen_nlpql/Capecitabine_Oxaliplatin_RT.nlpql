
// Phenotype library name
phenotype "Regimen for Capecitabine, Oxaliplatin, RT" version "1";

// Phenotype library description 
description "Generated query for cancer treatment based on https://hemonc.org/wiki/Main_Page";

// # Referenced libraries #
include ClarityCore version "1.0" called Clarity;

documentset Docs:
    Clarity.createReportTypeList(["Nursing/other", "Nursing", "Physician ", "Discharge summary", "Radiology",
         "General", "ECG", "Pathology", "Echo"]);

// Medication(s) inclusion criteria termset, if medication criteria present

termset TreatmentTerms_0:[
    "xeloda", "capecitabine"
];

define  Capecitabine_Treatment:
    Clarity.ProviderAssertion({
        termset:[TreatmentTerms_0],
        documentset:[Docs]
}); 

        
termset TreatmentTerms_1:[
    "eloxatin", "oxaliplatin"
];

define  Oxaliplatin_Treatment:
    Clarity.ProviderAssertion({
        termset:[TreatmentTerms_1],
        documentset:[Docs]
}); 

        
termset TreatmentTerms_2:[
    "external beam radiotherapy"
];

define  Externalbeamradiotherapy_Treatment:
    Clarity.ProviderAssertion({
        termset:[TreatmentTerms_2],
        documentset:[Docs]
}); 

        

// Regimen names

termset Regimen_Terms_Capecitabine_Oxaliplatin_RT:[
    "Capecitabine, Oxaliplatin, RT"
];

define final Regimen_Capecitabine_Oxaliplatin_RT:
  Clarity.ProviderAssertion({
    termset:[Regimen_Terms_Capecitabine_Oxaliplatin_RT],
    documentset:[Docs]
   }); 


// Results

        
define final Treatment_CapecitabineOxaliplatinRT:
    where Capecitabine_Treatment AND Oxaliplatin_Treatment AND Externalbeamradiotherapy_Treatment;
          
define final MatchedAllResults:
    where Treatment_CapecitabineOxaliplatinRT AND Capecitabine_Oxaliplatin_RT_Regimen;
        
        

// Comments

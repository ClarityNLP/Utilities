
// Phenotype library name
phenotype "Regimen for Mitotane & Streptozocin" version "1";

// Phenotype library description 
description "Generated query for cancer treatment based on https://hemonc.org/wiki/Main_Page";

// # Referenced libraries #
include ClarityCore version "1.0" called Clarity;

documentset Docs:
    Clarity.createReportTypeList(["Nursing/other", "Nursing", "Physician ", "Discharge summary", "Radiology",
         "General", "ECG", "Pathology", "Echo"]);

// Medication(s) inclusion criteria termset, if medication criteria present

termset TreatmentTerms0:[
    ":category:serotonin 5-ht3 antagonists", ":category:serotonin 5-ht3 antagonists"
];

define  CategorySerotonin5HT3antagonistsTreatment:
    Clarity.ProviderAssertion({
        termset:[TreatmentTerms0],
        documentset:[Docs]
}); 

        
termset TreatmentTerms1:[
    "cortef", "hydrocortisone"
];

define  HydrocortisoneTreatment:
    Clarity.ProviderAssertion({
        termset:[TreatmentTerms1],
        documentset:[Docs]
}); 

        

// Regimen names

termset RegimenTerms:[
    "Mitotane & Streptozocin"
];

define final MitotaneStreptozocinRegimen:
  Clarity.ProviderAssertion({
    termset:[RegimenTerms],
    documentset:[Docs]
   }); 


// Results

        
define final MitotaneStreptozocinTreatments:
    where CategorySerotonin5HT3antagonistsTreatment AND HydrocortisoneTreatment;
          
        
        
        

// Comments
/***

Known regimen for: Adrenocortical_carcinoma


***/
 

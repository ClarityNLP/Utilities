         "General", "ECG", "Pathology", "Echo"]);

// Medication(s) inclusion criteria termset, if medication criteria present

termset TreatmentTerms0:[
    "erivedge", "vismodegib"
];

define Treatment_Vismodegib:
    Clarity.ProviderAssertion({
        termset:[TreatmentTerms0],
        documentset:[Docs]
}); 

        

// Regimen names

termset RegimenTerms:[
    "Vismodegib_monotherapy", "Vismodegibmonotherapy", "Vismodegib monotherapy"
];

define Regimen_Vismodegibmonotherapy:
  Clarity.ProviderAssertion({
    termset:[RegimenTerms],
    documentset:[Docs]
   }); 


// Results

        
define final Received_Vismodegibmonotherapy:
    where Treatment_Vismodegib OR Regimen_Vismodegibmonotherapy;
            


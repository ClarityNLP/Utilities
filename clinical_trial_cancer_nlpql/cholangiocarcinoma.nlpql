
// Phenotype library name
phenotype "Clinical Trial Criteria related to Cholangiocarcinoma" version "1";

// Phenotype library description 
description "Generated query for cancer treatment based on https://hemonc.org/wiki/Main_Page";

// # Referenced libraries #
include ClarityCore version "1.0" called Clarity;

documentset Docs:
    Clarity.createReportTypeList(["Clinical Trial Inclusion Criteria", "Clinical Trial Exclusion Criteria"]);

// Cancers

termset Condition_Terms_cholangiocarcinoma:[
    "mouse cholangiocarcinoma", "cholangiocarcinoma, malignant", "cholangiocar.- intra/extrahepatic", "cc", "cholangiocarcinoma", "cholangiocarcinoma, intrahepatic and extrahepatic bile ducts (adenocarcinoma)", "intrahepatic bile duct cancer (cholangiocarcinoma)", "rat cholangiocarcinoma", "cholangiosarcoma", "bile duct carcinoma of mouse", "cholangiocarcinoma (reni)", "cholangiocellular carcinoma"
];

define final Condition_cholangiocarcinoma:
  Clarity.ProviderAssertion({
    termset:[Condition_Terms_cholangiocarcinoma],
    documentset:[Docs]
   }); 


